<!DOCTYPE html>
<html lang="en">
    <head>
        {{#meta}}
          <meta charset="utf-8">
          <meta http-equiv="X-UA-Compatible" content="IE=edge">
          <meta name="viewport" content="width=device-width, initial-scale=1">
          <meta name="description" content="{{description}}">
          <meta name="author" content="{{author}}">
        {{/meta}}
        <title>{{title}}</title>
        <!-- Bootstrap Core CSS -->
        <!-- Bootstrap Core CSS - Uses Bootswatch Flatly Theme: http://bootswatch.com/flatly/ -->
        <link href="https://bootswatch.com/3/flatly/bootstrap.min.css" rel="stylesheet">

        <!-- Custom CSS -->
        <link href="css/postcss.css" rel="stylesheet">

        <!-- Custom Fonts -->

        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ" crossorigin="anonymous">
        <link href="font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
        <!-- <link href="font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css"> -->
        <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
        <link href="http://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
          <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body id="page-top" class="index">

        <!-- Navitagion -->
        <nav class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <div class="navbar-header page-scroll">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-navbar-collapse-1">
                        <span class="sr-only">Totggle Navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#page-top">{{navBrand}}</a>
                </div>

                <!--Group for toggling-->
                <div class="collapse navbar-collapse" id="bs-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="hidden">
                            <a href="#page-top"></a>
                        </li>
                        {{#navBarToggling}}
                          <li class="page-scroll">
                              <a href="#{{id}}">{{text}}</a>
                          </li>
                        {{/navBarToggling}}
                    </ul>
                </div>
                <!--navbar-collapse-->
            </div>
            <!--containerfluid-->
        </nav>

        <header>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                      {{#profileIntro}}
                        <img class="img-responsive img-thumbnail" src="{{{img}}}" alt="">
                        <div class="intro-text">
                            <span class="name">{{name}}</span>
                            <hr class="star-light">
                            <span class="skills">{{description}}</span>
                        </div>
                      {{/profileIntro}}
                    </div>
                </div>
            </div>
        </header>

        <!--Portfolio Section-->
        <section id="portfolio">
          {{#portfolio}}
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h2>{{title}}</h2>
                        <hr class="star-primary">
                    </div>
                </div>
                {{#items}}
                  <div class="row">
                    <h2 class="display-2">Web Development</h2>
                    {{#webDevelopment}}
                          <div class="col-sm-4 portfolio-item">
                              <a href="#{{modalId}}" class="portfolio-link" data-toggle="modal">
                                  <div class="caption">
                                      <div class="caption-content">
                                          <i class="fa fa-search-plus fa-3x"></i>
                                      </div>
                                  </div>
                                  <img src="{{{display}}}" class="img-responsive img-centered" alt="">
                              </a>
                          </div>
                    {{/webDevelopment}}
                  </div>

                  <div class="row">
                    <h2 class="display-2">Mobile</h2>
                    {{#mobile}}
                          <div class="col-sm-4 portfolio-item">
                              <a href="#{{modalId}}" class="portfolio-link" data-toggle="modal">
                                  <div class="caption">
                                      <div class="caption-content">
                                          <i class="fa fa-search-plus fa-3x"></i>
                                      </div>
                                  </div>
                                  <img src="{{{display}}}" class="img-responsive img-centered" alt="">
                              </a>
                          </div>
                    {{/mobile}}
                  </div>
                {{/items}}
            </div>
          {{/portfolio}}
        </section>

        <!-- About section -->
        <section class="success working-xp" id="about">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h2 class="display-2">Working Experience</h2>
                        <hr class="star-light">
                    </div>
                </div>
                {{#workingXp}}
                <div class="row">
                    <h3 class="">{{company}}</h3>
                    <div class="">
                        <p>{{&brief}}</p>
                        <span>Role: {{role}}</span> <br>
                        <span>Date: {{dateFrom}} - {{dateTo}}</span>
                    </div>
                </div>
                {{/workingXp}}
            </div>
        </section>

        <!--Skills section-->
        <section id="skills">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h2>Skills</h2>
                        <hr class="star-primary">
                    </div>
                </div>
                {{#skills}}
                 <div class="skill">
                  <h3>{{name}}</h3>
                  <div class="row">
                    <ul class="list-inline">
                      {{#skillSet}}
                        <li>{{{.}}}</li>
                      {{/skillSet}}
                    </ul>
                  </div>
                 </div>
                {{/skills}}
            </div>
        </section>

        <!--End of modal portfolio section-->
        <footer class="text-center" id="contact">
            <div class="footer-above">
                <div class="container">
                    <div class="row">
                        <div class="footer-col col-md-6">
                            <h3>Location</h3>
                            <p>Santo Domingo, Dominican Republic</p>
                        </div>

                        <div class="footer-col col-md-6">
                            <h3>Keep in touch</h3>
                            <ul class="social-bar">
                            {{#contact}}
                             <li>{{{.}}}</li>
                            {{/contact}}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div class="footer-below">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <p>Theme provided by <a target="_blank" href="https://startbootstrap.com/">Start Bootstrap</a>, built by <a href="http://ernestomancebo.me">Ernesto Mancebo</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

        <!--Scroll to Top Button (for small and x-small screens)-->
        <div class="scroll-top page-scroll visible-xs visible-sm">
            <a class="btn btn-primary" href="#page-top">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>

        <!--Start modal portfolio section-->
        {{#projectsModals}}
          <div class="portfolio-modal modal fade" id="{{projectId}}" tabindex="-1" role="dialog" aria-hidden="true">
              <div class="modal-content">
                  <div class="close-modal" data-dismiss="modal">
                      <div class="lr">
                          <div class="rl"></div>
                      </div>
                  </div>

                  <div class="container">
                      <div class="row">
                          <div class="col-lg-8 col-lg-offset-2">
                              <div class="modal-body">
                                  <h2>{{title}}</h2>
                                  <hr class="star-primary">
                                  <img src="{{{thumbnail}}}" class="img-responsive img-centered" alt="">
                                  <p>{{brief}}</p>
                                  <div class="text-left">
                                      <br>
                                      <p class="">Services:</p>
                                      <ul>
                                      {{#services}}
                                          <li>
                                              {{.}}
                                          </li>
                                      {{/services}}
                                      </ul>
                                    </div>
                                  <div class="text-left">
                                  {{#date}}
                                    <p class="">Project Details:</p>
                                  {{/date}}
                                    <ul>
                                      {{#link}}
                                      <li>
                                        <span>Link: {{{.}}}</span>
                                      </li>
                                      {{/link}}
                                      {{#date}}
                                      <li>
                                        <span>Date: {{.}}</span>
                                      </li>
                                      {{/date}}
                                    </ul>
                                  </div>
                                  <button type="button" class="btn btn-default" data-dismiss="modal">
                                    <i class="fa fa-times"></i>Close
                                  </button>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
        {{/projectsModals}}

    <!-- jQuery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>

    <!-- Custom Theme JavaScript -->
    <script src="bundle.js"></script>

    </body>
</html>
